{
	"name": "parquet_processing",
	"properties": {
		"activities": [
			{
				"name": "Copy parquet file",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "ParquetSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"wildcardFileName": "*.parquet",
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "ParquetReadSettings"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "model",
									"type": "String",
									"physicalType": "UTF8"
								},
								"sink": {
									"name": "model",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "mpg",
									"type": "Double",
									"physicalType": "DOUBLE"
								},
								"sink": {
									"name": "mpg",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "cyl",
									"type": "Int32",
									"physicalType": "INT_32"
								},
								"sink": {
									"name": "cyl",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "disp",
									"type": "Double",
									"physicalType": "DOUBLE"
								},
								"sink": {
									"name": "disp",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "hp",
									"type": "Int32",
									"physicalType": "INT_32"
								},
								"sink": {
									"name": "hp",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "drat",
									"type": "Double",
									"physicalType": "DOUBLE"
								},
								"sink": {
									"name": "drat",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "wt",
									"type": "Double",
									"physicalType": "DOUBLE"
								},
								"sink": {
									"name": "wt",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "qsec",
									"type": "Double",
									"physicalType": "DOUBLE"
								},
								"sink": {
									"name": "qsec",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "vs",
									"type": "Int32",
									"physicalType": "INT_32"
								},
								"sink": {
									"name": "vs",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "am",
									"type": "Int32",
									"physicalType": "INT_32"
								},
								"sink": {
									"name": "am",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "gear",
									"type": "Int32",
									"physicalType": "INT_32"
								},
								"sink": {
									"name": "gear",
									"type": "String",
									"physicalType": "String"
								}
							},
							{
								"source": {
									"name": "carb",
									"type": "Int32",
									"physicalType": "INT_32"
								},
								"sink": {
									"name": "carb",
									"type": "String",
									"physicalType": "String"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "src_parquet",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "parquet_sink",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "Parquetprocessing"
		},
		"annotations": []
	}
}